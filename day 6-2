#include<iostream>
using namespace std;

int
main ()
{
  
int t;
  
cin >> t;
  
int i = 0;
  
while (i < t)
    {
      
int N,count=0;
cin>>N;
char d[N];
cin>>d;
for(int i=0;i<N;i++){
    if(d[i]=='5'||d[i]=='0'){
        cout<<"yes";
        break;
    }
    count++;
}
if(count==N){
    cout<<"no";
}

      
i++;
      

}

}

#include<iostream>
using namespace std;
int main()
{
    int t;
    
    cout<<"enter test cases to perform: ";
    cin>>t;
    
    for(int i=1; i<=t; i++)
    {
        int x,y;
        cin>>x>>y;
    
        if(x>10)
        {
            cout<<"yes"<<endl;
        }
        else
        {
            cout<<"no"<<endl;
        }
    }
}

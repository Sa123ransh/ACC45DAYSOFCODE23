#include<iostream>
using namespace std;
int main()
{
    int t;
    cin>>t;
    
    for(int i=1; i<=t; i++)
    {
        int A,B,X,Y;
        cin>>A>>B>>X>>Y;
        
        if(A*B<=X*Y)
        {
            cout<<"YES"<<endl;
        }else
        {
            cout<<"NO"<<endl;
        }
    }
    return 0;
}

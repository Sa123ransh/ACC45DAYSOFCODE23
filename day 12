#include<iostream>
using namespace std;
int main()
{
    int t;
    cin>>t;
    
    
    for(int i=0; i<t; i++)
    {
        
       int x,y,z;
       cin>>x>>y>>z;
       
       if((y/x)>z)
       {
           cout<<0<<endl;
       }
       else
       {
           cout<<z-(y/x)<<endl;
       }
       
       
       
    }
    return 0;
    
}

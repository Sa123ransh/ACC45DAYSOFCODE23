#include<iostream>
using namespace std;
int
main ()
{
  
int t;
  
cin >> t;
  
int i = 0;
  
while (i < t)
    {
      
int A,B;
cin>>A>>B;
if(A>B){
    int c=7-A;
    if(c<0){
        cout<<0<<endl;
    }
    else{
        cout<<c<<endl;
    }
    
}

else{
   int c=7-B;
    if(c<0){
        cout<<0<<endl;
    }
    else{
        cout<<c<<endl;
    }
}

      

i++;

}

}


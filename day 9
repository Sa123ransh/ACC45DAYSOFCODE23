#include<iostream>
using namespace std;
int main()
{
    int t;
    cout<<"enter test cases to perform: ";
    cin>>t;
    
    for(int i=0; i<=t; i++)
    {
        int x,y;
        cin>>x>>y;
        
            if(x==y)
            {
                cout<<0<<endl;
            }
            else
            {
                cout<<y/x<<endl;
            }
    }
    return 0;
}

